<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <requires lib="Adw" version="1.2"/>
  <template class="InstallWindow" parent="GtkStack">
    <style>
      <class name="window"/>
    </style>
    <property name="halign">center</property>
    <property name="valign">center</property>
    <property name="transition-type">slide-left</property>
    <property name="vexpand">true</property>
    <property name="hexpand">true</property>
    <child>
      <object class="DiskSelect" id="Disk_select">
      </object>
    </child>
    <child>
      <object class="InstallProgress" id="Install_progress">
      </object>
    </child>
    <child>
      <object class="InstallFail" id="InstallFail">
      </object>
    </child>
    <child>
      <object class="InstallFinish" id="InstallFinish">
      </object>
    </child>
  </template>
</interface>
